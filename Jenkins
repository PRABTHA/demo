pipeline {
    agent any

    stages {
        stage('Git Clone') {
            steps {
                echo 'clonning the repository'
            }
        }    
            
         stage('Build') {
            steps {
                echo 'Building the code'
            } 
        }  
        
        
      stage('Test') {
            steps {
                echo 'Testing the code'
            } 
        } 
       
       
       
       stage('Deploy') {
            steps {
                echo 'Deploying the code'
            } 
        } 
      
      
        
    }
}
